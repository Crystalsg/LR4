def sum_of_n_digit_numbers(n):
    first_number = 10**(n-1)
    last_number = 10**n - 1
    count = last_number - first_number + 1
    sum_n = count * (first_number + last_number) // 2
    return sum_n

# Calculate the sum for n = 1 to 4
sums = {n: sum_of_n_digit_numbers(n) for n in range(1, 5)}
sums
